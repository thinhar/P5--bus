%=============Templating=============%
%% Website
%% site:kort navn, skal være unikt
%% Fuldt artikelnavn
%% Udgivelsesmåned
%% Udgivelsesår
%% URL
%% Dato hvorpå den er indsamlet. HUSK DET.
% Optional fields: author, title, howpublished, month, year, note

%% bruges til technologiske rapporter (aka academiske rapporter %%
@techreport{label,
	author = {author},	
	title = {title},
	year = {year},
	month = {month},
	address = {\url{url}},
    note = {Visited: date}
}
%% Bruges som en go to label til udspecificerede kilder %%
@manual{Label,
    author = {author},
	title = {title},
	year = {year},
	month = {month},
	address = {\url{url}},
    note = {Visited: date}
}
%% bruges til artikler, fra journaler (aka Aviser) %%
@article{Label,
	author = {author},
	title = {title},
    journal = {journal},
    year = {year},
	volume = {Artikel id: id number},
	pages = {page number},
    note = {Visited: date}
}
%% bruges til bøger %%
@inbook{label,
	author = {author},
    title = {title},
    year = {year},
    chapter = {chapter number},
    pages = {page number},
    address = {\url{url}},
    note = {Visited: date}
}
